lista = [50, 40, 30, 20, 10]

def invertir_lista(lista):
  if lista == []:
    inversa = lista
  else:
    inversa = [lista[-1]] + invertir_lista(lista[:-1])
  return inversa  
print(invertir_lista(lista))
    
